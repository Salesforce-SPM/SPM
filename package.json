{
  "name": "spm",
  "version": "0.0.0",
  "author": "Gabriel Loch",
  "bin": {
    "spm": "./bin/run"
  },
  "bugs": "https://github.com/salesforce-package-manager-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "axios": "^0.21.1",
    "chalk": "^4.1.1",
    "cli-ux": "^5.6.3",
    "decompress": "^4.2.1",
    "inquirer": "^8.1.1",
    "inquirer-search-checkbox": "^1.0.0",
    "inquirer-search-list": "^1.2.6",
    "jsforce": "^1.10.1",
    "tslib": "^1.14.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/decompress": "^4.2.4",
    "@types/inquirer": "^7.3.1",
    "@types/jsforce": "^1.9.33",
    "@types/node": "^10.17.60",
    "dotenv": "^10.0.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "gulp": "^4.0.2",
    "npm-check": "^5.9.2",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/salesforce-package-manager/salesforce-package-manager-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "spm",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "environment": {
        "description": "Environments commands, see spm environmen -h for more"
      },
      "project": {
        "description": "Project commands, see spm project -h for more"
      }
    }
  },
  "repository": "salesforce-package-manager/salesforce-package-manager-cli",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md",
    "gulp:new-command": "npx gulp newCommand"
  },
  "types": "lib/index.d.ts"
}
